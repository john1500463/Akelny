<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Admin.EditRestaurant"
    tools:layout_editor_absoluteY="81dp">

    <EditText
        android:id="@+id/EndTimeEdit"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="36dp"
        android:layout_marginRight="36dp"
        android:ems="10"
        android:hint="End Time"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toBottomOf="@+id/StartTimeEdit"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/StartTimeEdit"
        app:layout_constraintTop_toTopOf="@+id/StartTimeEdit" />

    <EditText
        android:id="@+id/LatitudeEdit"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="36dp"
        android:layout_marginRight="36dp"
        android:ems="10"
        android:hint="Latitude"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toBottomOf="@+id/LongitudeEdit"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/LongitudeEdit"
        app:layout_constraintTop_toTopOf="@+id/LongitudeEdit" />

    <EditText
        android:id="@+id/StartTimeEdit"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="20dp"
        android:layout_marginEnd="34dp"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="34dp"
        android:layout_marginStart="16dp"
        android:ems="10"
        android:hint="StartTime"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/LongitudeEdit"
        app:layout_constraintEnd_toStartOf="@+id/EndTimeEdit"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/DeleviryFeesEdit" />

    <EditText
        android:id="@+id/LongitudeEdit"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="14dp"
        android:layout_marginEnd="34dp"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="34dp"
        android:layout_marginStart="16dp"
        android:ems="10"
        android:hint="Longitude"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/LogoEdit"
        app:layout_constraintEnd_toStartOf="@+id/LatitudeEdit"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/StartTimeEdit" />

    <EditText
        android:id="@+id/DeleviryFeesEdit"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="21dp"
        android:layout_marginEnd="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp"
        android:layout_marginStart="16dp"
        android:ems="10"
        android:hint="Deleviry Fees"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/StartTimeEdit"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/DeleviryTimeEdit" />

    <EditText
        android:id="@+id/DeleviryTimeEdit"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="21dp"
        android:layout_marginEnd="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp"
        android:layout_marginStart="16dp"
        android:ems="10"
        android:hint="Deleviry Time"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/DeleviryFeesEdit"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/EditResturantSpinner" />

    <Button
        android:id="@+id/EditRestaurantToDataBase"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="26dp"
        android:text="Edit Restaurant"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/LogoEdit" />

    <Button
        android:id="@+id/LogoEdit"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="26dp"
        android:layout_marginEnd="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp"
        android:layout_marginStart="16dp"
        android:text="Logo Image Change"
        app:layout_constraintBottom_toTopOf="@+id/EditRestaurantToDataBase"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/LongitudeEdit" />

    <Spinner
        android:id="@+id/EditResturantSpinner"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="22dp"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="29dp"
        app:layout_constraintBottom_toTopOf="@+id/DeleviryTimeEdit"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</android.support.constraint.ConstraintLayout>